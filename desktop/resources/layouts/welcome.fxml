<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <TitledPane fx:id="titledPane" collapsible="false" text="Welcome">
               <content>
                  <VBox spacing="8.0">
                     <padding>
                        <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
                     </padding>
                     <children>
                        <Label text="Mallard">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                        <Region prefHeight="16.0" />
                        <Hyperlink fx:id="newProjectLink" onAction="#onNewProjectAction" text="Start a new project" />
                        <Hyperlink fx:id="existingProjectLink" layoutX="34.0" layoutY="101.0" onAction="#onExistingProjectAction" text="Open an existing project" />
                     </children>
                  </VBox>
               </content>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </TitledPane>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </VBox>
   </children>
</HBox>
